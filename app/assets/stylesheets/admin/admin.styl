label.checkbox
	display inline-block
	height 34px
	line-height 34px
	position relative
	user-select(none)
	input
		display none
	> div
		display inline-block
		vertical-align middle
		position absolute
		top 8px
		left 16px
		width 15px
		height 15px
		background-color rgba(255, 255, 255, 0.8)
		border 2px solid #444
		font-size 13px
		cursor pointer
		font-family 'icomoon'
		transition(unquote("all 100ms cubic-bezier(0.23, 1, 0.32, 1) 0ms"))
		border-radius(3px)
	> div:after
		content "\e2aa"
		width 15px
		height 15px
		line-height 15px
		position absolute
		left 0
		background-color #00BCD4
		color white
		opacity 0
		transform(scale(0))
		transition(unquote("opacity 450ms cubic-bezier(0.23, 1, 0.32, 1) 0ms, transform 0ms cubic-bezier(0.23, 1, 0.32, 1) 450ms"))
	div.checked
		border-color #00BCD4
		transition(unquote("all 100ms cubic-bezier(0.23, 1, 0.32, 1) 0ms"))
	div.checked:after
		opacity 1
		transform(scale(1))
		transition(unquote("opacity 0ms cubic-bezier(0.23, 1, 0.32, 1) 0ms, transform 800ms cubic-bezier(0.23, 1, 0.32, 1) 0ms"))
