//= require_self
//= require_tree ./styles

@font-face
  font-family 'Droid Sans'
  font-style normal
  font-weight 400
  src unquote("local('Droid Sans'), local('DroidSans'), url('http://fonts.gstatic.com/s/droidsans/v6/s-BiyweUPV0v-yRb-cjciPk_vArhqVIZ0nv9q090hN8.woff2') format('woff2')")
  unicode-range unquote("U+0000-00FF, U+0131, U+0152-0153, U+02C6, U+02DA, U+02DC, U+2000-206F, U+2074, U+20AC, U+2212, U+2215, U+E0FF, U+EFFD, U+F000")

@import "./styles/mixins"

@keyframes animation-notify {
  0% {
  	opacity: 0;
  	transform: scale3d(.3, .3, .3);
  }
  30% {
  	opacity: 1;
  	top: 30px;
  	transform: scale3d(1, 1, 1);
  }
  90% {
  	opacity: 1;
  	top: 30px;
  	transform: scale3d(1, 1, 1);
  }
  100% {
  	opacity: 0;
  	top: -30px;
  	transform: scale3d(.3, .3, .3);
  }
}

@keyframes spin {
  0% {
  	transform(rotate(0deg))
  }
  50% {
  	transform(rotate(180deg))
  }
  100% {
  	transform(rotate(360deg))
  }
}

